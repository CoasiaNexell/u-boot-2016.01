/dts-v1/;
#include "s5p6818.dtsi"
#include "s5p6818-vapor-gang-nxe2000.dtsi"

/ {
	model = "Vapor Gang Write board based on Nexell s5p6818";
	cpu-model = "S5p6818";

	compatible = "nexell,vapor-gang", "nexell,s5p6818";

	mmc0:mmc@c0062000 {
		frequency = <50000000>;
		nexell,drive_dly = <0x0>;
		nexell,drive_shift = <0x03>;
		nexell,sample_dly = <0x00>;
		nexell,sample_shift = <0x02>;
		status = "okay";
	};

	mmc2:mmc@c0069000 {
		frequency = <50000000>;
		nexell,drive_dly = <0x0>;
		nexell,drive_shift = <0x03>;
		nexell,sample_dly = <0x00>;
		nexell,sample_shift = <0x02>;
		nexell,bus-width = <8>;
		index = <2>;
		status = "okay";
	};

	ehci:usbhost@c0030000 {
		status = "okay";
	};

	dwc2otg@c0040000 {
		status = "okay";
	};

	nxe2000_gpio:i2c_gpio@0 {
		compatible = "i2c-gpio";
		#address-cells = <1>;
		#size-cells = <0>;
		gpios = <&gpio_e 8 1>,	/* SDA */
		      <&gpio_e 9 1>;	/* SCL */

		i2c-gpio,delay-us = <3>;
		nxe2000_gpio@32 {
			compatible = "nexell,nxe2000";
			reg = <0x32>;
		};
	};

	nx_i2c_gpio:i2c_gpio@5 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "i2c-gpio";
		gpios = <&gpio_c 27 0>,
			<&gpio_c 25 0>;
		i2c-gpio,delay-us = <1>;
		nx_i2c_gpio@44 {
			compatible = "nexell,nx_i2c_gpio";
			reg = <0x44>;
			sda_pad = <1>;
			scl_pad = <1>;
		};
	};

	dp0:dp@c0102800 {
	        status = "okay";
	        module = <0>;
	        lcd-type = "hdmi";

		dp-device {
			format = <0>;	//	0:VESA, 1:JEIDA
			preset = <1>;	//	0:1280x720, 1: 1920x1080
		};
		dp-sync {
			h_active_len = <1920>;
			h_front_porch = <30>;
			h_back_porch = <30>;
			h_sync_width = <4>;
			v_active_len = <1080>;
			v_front_porch = <10>;
			v_back_porch = <10>;
			v_sync_width = <2>;
		};
		dp-ctrl {
			clk_src_lv0 = <3>;
			clk_div_lv0 = <10>;
			clk_src_lv1 = <7>;
			clk_div_lv1 = <1>;
			out_format = <3>;
		};
		dp-planes {
			layer_top {
				screen_width = <1920>;
				screen_height = <1080>;
				back_color = <0x0>;
			};
			layer_1 {
				fb_base = <0x7E000000>;
				width = <1920>;
				height = <1080>;
				format = <0x06530000>;
				pixel_byte = <4>;
			};
		};
	};
};
